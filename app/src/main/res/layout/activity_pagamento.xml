<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/header"
    tools:context=".Pagamento">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="599dp"
        android:background="@color/fundo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="16dp"
            android:text="Forma de Pagamento"
            android:textSize="34sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="363dp"
            android:layout_height="502dp"
            android:background="@color/fundoSobretom"
            tools:layout_editor_absoluteX="27dp"
            tools:layout_editor_absoluteY="65dp">

            <Button
                android:id="@+id/btnCadastro2"
                style="@style/Widget.Material3.Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:backgroundTint="@color/botãoSecundário"
                android:text="Confirmar"
                android:textColor="@color/fonteRodapé"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.461"
                app:layout_constraintStart_toEndOf="@+id/btnLogin2"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                app:layout_constraintVertical_bias="1.0" />

            <Button
                android:id="@+id/btnLogin2"
                style="@style/Widget.Material3.Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:layout_marginBottom="16dp"
                android:text="Cancelar"
                android:textColor="@color/fonteRodapé"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.207"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtSenha" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="316dp"
                android:layout_height="64dp"
                android:background="@color/header"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="277dp">

                <Button
                    android:id="@+id/button5"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="123dp"
                    android:layout_height="66dp"
                    android:background="@color/botão"
                    android:text="Preço"
                    android:textSize="16sp"
                    tools:layout_editor_absoluteX="-2dp"
                    tools:layout_editor_absoluteY="-1dp" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="36dp"
                    android:layout_marginTop="16dp"
                    android:text="R$000,00"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/button5"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <EditText
                android:id="@+id/editTextTextPersonName8"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="128dp"
                android:layout_marginTop="8dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5" />

            <EditText
                android:id="@+id/editTextTextPersonName9"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="236dp"
                android:layout_marginTop="12dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName6" />

            <EditText
                android:id="@+id/editTextTextPersonName7"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName4" />

            <EditText
                android:id="@+id/editTextTextPersonName"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="319dp"
                android:layout_height="54dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Forma de Pagamento"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editTextTextPersonName2"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="213dp"
                android:layout_height="50dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

            <EditText
                android:id="@+id/editTextTextPersonName3"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="236dp"
                android:layout_marginTop="12dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

            <EditText
                android:id="@+id/editTextTextPersonName4"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

            <EditText
                android:id="@+id/editTextTextPersonName5"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="128dp"
                android:layout_marginTop="12dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

            <EditText
                android:id="@+id/editTextTextPersonName6"
                style="@style/Widget.Material3.AutoCompleteTextView.FilledBox"
                android:layout_width="100dp"
                android:layout_height="49dp"
                android:layout_marginStart="236dp"
                android:layout_marginTop="12dp"
                android:background="@color/header"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="@color/fonteAzul"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="317dp"
                android:layout_height="86dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Quis ipsum suspendisse ultrices gravida. Risus commodo viverra maecenas accumsan lacus vel facilisis."
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>